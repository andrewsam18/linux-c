cat /etc/shells
 /etc/shells: valid login shells
/bin/sh
/bin/bash
/usr/bin/bash
/bin/rbash
/usr/bin/rbash
/bin/dash
/usr/bin/dash
/usr/bin/sh

echo $SHELL
/bin/bash
cat > helloworld.sh
#!/bin/bash
echo "Hello World, iam learning scripting"
ls -l helloworld.sh
-rw-rw-r-- 1 welcome welcome 54 Mar 20 01:36 helloworld.sh
helloworld.sh
